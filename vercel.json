{
    "version": 2,
    "builds": [
        {
            "src": "dist/src/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["dist/src/**", "dist/drizzle/**"],
                "buildCommand" : ["npm run generate && npm run push"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/src/index.js"
        }
    ]
}